<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <queries>
        <intent>
            <action android:name="android.intent.action.VIEW" />

            <data android:scheme="https" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IntheBeningin"
        tools:targetApi="31">
        <activity
            android:name=".activity.ex10.Ex10Activity"
            android:exported="true">
        <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".activity.ex9.Ex9Activity"
            android:exported="false">

        </activity>
        <activity
            android:name=".activity.ex8.Ex8Activity"
            android:exported="false" />
        <activity
            android:name=".activity.ex6.Ex6Activity"
            android:exported="false" />
        <activity
            android:name=".activity.ex5.Ex5Activity"
            android:exported="false" />
        <activity
            android:name=".activity.ex3.AfterlifeActivity"
            android:exported="false"
            android:parentActivityName=".activity.ex3.MainActivity" />
        <activity
            android:name=".activity.ex3.MainActivity"
            android:exported="false" />
    </application>

</manifest>